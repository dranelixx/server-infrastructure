{{- with secret "secret/data/prod/services/pms-prod-cz-01/plex" -}}
# Plex connection (localhost - no DNS/SSL needed)
PLEX_URL=http://127.0.0.1:32400
PLEX_TOKEN={{ .Data.data.token }}
{{- end }}
PLEX_PATH=/plex

# Operation mode
MODE=report
TIMEOUT=600
SLEEP=60

# Cleanup tasks
PHOTO_TRANSCODER=True
EMPTY_TRASH=True
CLEAN_BUNDLES=True
OPTIMIZE_DB=True
